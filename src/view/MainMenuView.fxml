<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainMenuController">
    <children>
        <Button fx:id="rentBikeButton" layoutX="218.0" layoutY="110.0" text="Забронировать велосипед" onAction="#showBookingBikeView" />
        <Button fx:id="returnBikeButton" layoutX="234.0" layoutY="146.0" text="Личная информация" />
        <Button fx:id="viewBikesButton" layoutX="250.0" layoutY="181.0" onAction="#showMyOrdersView" text="Мои заказы" />
      <Button fx:id="exitButton" layoutX="266.0" layoutY="286.0" onAction="#exit" text="Выйти">
         <font>
            <Font name="Gilroy-Regular" size="12.0" />
         </font></Button>
    </children>
</AnchorPane>
